generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Address {
  id          Int      @id @default(autoincrement())
  displayName String   @map("display_name")
  lat         Float
  lon         Float
  createdAt   DateTime @default(now()) @map("created_at")
}
